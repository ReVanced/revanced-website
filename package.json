{
	"name": "revanced-website",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run build-docusaurus",
		"build-docusaurus": "cd docusaurus && npm build && npx ncp build ../public/docs",
		"package": "svelte-kit package",
		"preview": "sirv ./public --no-clear",
		"vite:preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@types/marked": "^4.0.7",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"imagetools-core": "^3.2.3",
		"ncp": "^2.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.55.0",
		"semver": "^7.5.4",
		"sirv-cli": "^2.0.2",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-meta-tags": "^2.8.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vite-imagetools": "^4.0.11"
	},
	"type": "module",
	"dependencies": {
		"@tanstack/query-core": "^4.24.4",
		"@tanstack/query-persist-client-core": "^4.24.4",
		"@tanstack/query-sync-storage-persister": "^4.24.4",
		"@tanstack/svelte-query": "^4.24.4",
		"asciidoctor": "^2.2.6",
		"marked": "^4.1.1"
	}
}